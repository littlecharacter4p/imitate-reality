<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans.xsd
		http://code.alibabatech.com/schema/dubbo
		http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

	<dubbo:application name="imitate-reality-consumer"></dubbo:application>

	<dubbo:registry protocol="zookeeper"
					address="zk1:2181,zk2:2181,zk3:2181"
					group="imitate-reality"/>

	<dubbo:protocol name="dubbo" port="29051" />

	<dubbo:reference id="userService" interface="com.lc.imitate.reality.contract.UserService" check="false" />
</beans>  